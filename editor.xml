<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_INTENTION_PREVIEW" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bTMP\b.*">
      <option name="FOREGROUND" value="27c065" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="27c065" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bxxx\b.*">
      <option name="FOREGROUND" value="c63923" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ab1008" />
    </pattern>
  </component>
</application>