<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add New Row">
        <Item>
          <IdeaActionID>Console.TableResult.AddRow</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>Add New Row</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'⌘[' or '⌥⌘←' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Close">
        <Item>
          <ShortCut>'⌘W'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'C2C'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'C2C'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Gradle'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'Gradle'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>15</Count>
        </Item>
      </Statistic>
      <Statistic Action="Down the stack trace">
        <Item>
          <ShortCut>'⌥⌘↓'</ShortCut>
          <Description>Down the stack trace</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Edit">
        <Item>
          <IdeaActionID>Console.TableResult.EditValue</IdeaActionID>
          <ShortCut>'F2' or '⏎' or '⌥⏎'</ShortCut>
          <Description>Edit</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide">
        <Item>
          <ShortCut>'⇧⎋'</ShortCut>
          <Description>Hide</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rebase...">
        <Item>
          <IdeaActionID>Git.Rebase</IdeaActionID>
          <ShortCut>'⌃⌘B'</ShortCut>
          <Description>Rebase...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload Page">
        <Item>
          <IdeaActionID>Console.TableResult.Reload</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Reload Page</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Gradle'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Gradle'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'build'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'build'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>DatabaseView.SynchronizeAction</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Up the stack trace">
        <Item>
          <ShortCut>'⌥⌘↑'</ShortCut>
          <Description>Up the stack trace</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="VCS Operations Popup...">
        <Item>
          <IdeaActionID>Vcs.QuickListPopupAction</IdeaActionID>
          <ShortCut>'⌃V'</ShortCut>
          <Description>VCS Operations Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>